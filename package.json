{
  "name": "todo-api",
  "version": "1.0.0",
  "type": "module",
  "description": "TODO App API with Session Authentication",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
